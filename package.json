		"eslint-plugin-svelte": "^2.30.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"plop": "^4.0.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.5.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.5.3",
