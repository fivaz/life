{
  "name": "life",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev --filter=!docs",
    "watch": "turbo run watch --filter=!docs",
    "build": "turbo run build --filter=!docs",
    "lint": "turbo run lint --filter=!docs",
    "format": "turbo run format --filter=!docs",
    "prune": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "turbo": "^2.4.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
