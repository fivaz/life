		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"plop": "^4.0.1",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
