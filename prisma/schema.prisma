generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model event {
  id          Int       @id @default(autoincrement())
  name        String
  description String?
  startDate   DateTime @db.Timestamp(6)
  endDate     DateTime @db.Timestamp(6)
  isDone      Boolean
}
