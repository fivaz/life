{
	"name": "svelendar",
	"version": "1.1.0",
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fivaz/life.git",
		"directory": "packages/svelendar"
	},
	"scripts": {
		"dev": "svelte-package --watch",
		"standlone:dev": "vite dev",
		"build": "vite build && npm run package",
		"dev:css:watch": "pnpm dlx @tailwindcss/cli -i src/app.css -o ./dist/styles.css --watch",
		"build:css": "pnpm dlx @tailwindcss/cli -i src/app.css -o ./dist/styles.css --minify",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && pnpm run build:css && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write . && eslint . --fix && pnpm run check",
		"lint": "prettier --check . && eslint .",
		"storybook": "storybook dev -p 6008",
		"build-storybook": "storybook build"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		"./styles.css": "./dist/styles.css",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"chart.js": "^4.4.6",
		"lucide-svelte": "^0.456.0"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.3",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"semantic-release-monorepo": "^8.0.2",
		"semantic-release": "^25.0.2",
		"@chromatic-com/storybook": "^4.1.3",
		"@fontsource/fira-mono": "^5.2.7",
		"@life/config-eslint": "workspace:*",
		"@life/config-prettier": "workspace:*",
		"@life/shared": "workspace:*",
		"@lucide/svelte": "^0.562.0",
		"@playwright/test": "^1.57.0",
		"@storybook/addon-a11y": "^10.1.8",
		"@storybook/addon-docs": "^10.1.8",
		"@storybook/addon-essentials": "^8.4.5",
		"@storybook/addon-interactions": "^8.4.5",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-themes": "^10.1.8",
		"@storybook/addon-vitest": "^10.1.8",
		"@storybook/blocks": "^8.4.5",
		"@storybook/svelte": "^8.4.5",
		"@storybook/sveltekit": "^10.1.8",
		"@storybook/test": "^8.4.5",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.15",
		"@vitest/coverage-v8": "^4.0.15",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dompurify": "^3.2.5",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^16.5.0",
		"interactjs": "^1.10.27",
		"playwright": "^1.57.0",
		"postcss": "^8.4.49",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.2.0",
		"storybook": "10.1.10",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"svelte-portal": "^2.2.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.0.0",
		"vite": "^7.2.6",
		"vitest": "^4.0.15",
		"vitest-browser-svelte": "^2.0.1"
	}
}
